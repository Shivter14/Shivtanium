	if !win[%%~1]H! lss 3 set "win[%%~1]H=3"
	set /a "win[%%~1]X=win[%%~1]X, win[%%~1]Y=win[%%~1]Y, win[%%~1]W=win[%%~1]W, win[%%~1]RH=(win[%%~1]H=win[%%~1]H)-1, win[%%~1]BX=!win[%%~1]X!+!win[%%~1]W!-1", "win[%%~1]BY=!win[%%~1]Y!+!win[%%~1]H!-1", "temp.tl=0"
	
	set "win[%%~1]title= %%~6"
