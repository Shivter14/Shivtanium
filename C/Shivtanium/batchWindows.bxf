:sleep
set exit=
for /l %%A in (1 1 10000) do if not defined exit (
	ping -n 2 127.0.0.1 > nul
	set /p "input="
	if "!input!"=="¤CTRL	RESUME" set exit=True
)
if not defined exit goto sleep
set exit=
exit /b 0
